#include <stdio.h>
//Pide al usuario dos números decimales y muestra el resultado de la división.
int main() {
	float a,b,division;
	printf("Ingrese primer numero decimal\n");
	scanf ("%f", &a);
	printf("Ingrese segundo numero decimal\n");
	scanf ("%f", &b);
	division=a/b;
	printf("La division de a/b será: %.2f",division);
	return 0;
}
