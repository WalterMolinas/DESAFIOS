#include <stdio.h>

int main() {
	int arr[] = {5, 2, 9, 1, 6};
	int n = sizeof(arr) / sizeof(arr[0]);
	int i, j, minIndex, temp;
	
	for (i = 0; i < n - 1; i++) {
		minIndex = i;
		for (j = i + 1; j < n; j++) {
			if (arr[j] < arr[minIndex]) {
				minIndex = j;
			}
		}
		// Intercambiar mÃ­nimo con el primer elemento no ordenado
		temp = arr[i];
		arr[i] = arr[minIndex];
		arr[minIndex] = temp;
	}
	
	printf("Arreglo ordenado: ");
	for (i = 0; i < n; i++) {
		printf("%d ", arr[i]);
	}
	
	return 0;
}
